{
    "version": 2,
    "builds": [
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      },
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/backend/(.*)",
        "dest": "backend/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }